{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=RhDev.Customer.Component.App;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "App": {
    "Timeout": -1
  },
  "Common": {
    "PrivacySettings": {
      "LogClientIpAddresses": true,
      "LogClientAgents": true
    },
    "OptionsReloadInSeconds": 120,
    "Identity": {
      "CookieExpirationInMinutes": 100,
      "SignoutInactivityIntervalInMinutes": 100
    },
    "CacheConfiguration": {
      "ExpirationDurationInMinutes": 1400
    },
    "QueueService": {
      "QueueCapacity": 1000
    },
    "SmsGate": {
      "Address": "127.0.0.1",
      "Port": 2020,
      "ByPassFailure": true
    },
    "SmtpServer": {
      "Address": "exchange.contoso.cz",
      "Sender": "admin@contoso.cz",
      "ByPassFailure": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "MudBlazor": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Hangfire.BackgroundJobServer": "Error",
        "Hangfire.Server.BackgroundServerProcess": "Error",
        "Hangfire.Server.ServerHeartbeatProcess": "Error",
        "Hangfire.Processing.BackgroundExecution": "Error",
        "BlazorState.Pipeline.State.CloneStateBehavior": "Error",
        "BlazorState.Store": "Error",
        "BlazorState.Subscriptions": "Error",
        "BlazorState.Pipeline.RenderSubscriptions.RenderSubscriptionsPostProcessor": "Error",
        "Microsoft.AspNetCore.Components.Web.ErrorBoundary": "Error"
      }
    },
    "Properties": {
      "Application": "Jobs.Test"
    }
  },
  "AllowedHosts": "*"
}
